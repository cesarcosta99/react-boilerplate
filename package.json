{
  "name": "react-boilerplate",
  "version": "1.0.1",
  "description": "Basic boilerplate and tooling for React application development - including Webpack2 and Babel",
  "repository": {
    "type": "git",
    "url": "https://github.com/cesarcosta99/react-boilerplate.git"
  },
  "main": "app.bundle.js",
  "license": "MIT",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "html-webpack-plugin": "^2.28.0",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^2.6.1"
  },
  "scripts": {
    "build:dev": "./node_modules/webpack/bin/webpack.js --env=dev --progress",
    "build:prod": "./node_modules/webpack/bin/webpack.js --env=prod --progress",
    "start": "./node_modules/webpack-dashboard/bin/webpack-dashboard.js ./node_modules/webpack-dev-server/bin/webpack-dev-server.js"
  },
  "keywords": [
    "react",
    "boilerplate",
    "webpack2",
    "babel"
  ]
}
